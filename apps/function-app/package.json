{
  "name": "@shellicar-core-foundation/function-app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/function-*.mjs",
  "scripts": {
    "deploy:dev": "dotenvx run -f ../../.env -- ./deploy.sh",
