    "@shellicar/build-graphql": "^1.2.2",
    "@shellicar/build-version": "^1.3.1",
    "@types/node": "^25.0.3",
    "esbuild": "^0.27.2",
    "glob": "^13.0.0",
    "npm-run-all2": "^8.0.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
