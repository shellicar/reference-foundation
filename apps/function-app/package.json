    "@shellicar/build-graphql": "^1.0.1",
    "@shellicar/build-version": "^1.1.0",
    "@types/node": "^22.15.16",
    "esbuild": "^0.25.4",
    "glob": "^11.0.2",
    "npm-run-all2": "^8.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
