{
  "name": "@shellicar-core-foundation/ui-config",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsup-node",
    "check": "tsc --noEmit --composite false --skipLibCheck",
    "dev": "tsup-node --watch"
  },
  "exports": {
    "./app.css": {
      "import": "./src/app.css"
    },
    "./postcss.config": {
      "types": "./src/postcss.config.ts",
      "import": "./dist/postcss.config.mjs",
      "require": "./dist/postcss.config.js"
    },
    "./tailwind.config": {
      "types": "./src/tailwind.config.ts",
      "import": "./dist/tailwind.config.mjs",
      "require": "./dist/tailwind.config.js"
    }
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tailwindcss": "^3.4.17"
  }
}
